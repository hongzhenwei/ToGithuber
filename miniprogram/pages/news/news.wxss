/* pages/timeline/timeline.wxss */

.pure_top{
  position: fixed;
  top:0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: -1;
  overflow: hidden;
background: linear-gradient(160deg,#1496f1, #E0F0FA);}

.pure_top::after{
  content: "";
  width: 150%;
  height: 50%;
  position: absolute;
  right: -50%;
  bottom:0;
  z-index: 1;
  border-radius: 70% 20% 0 0;
  background: white
}

.pure_top::before{
  content: "";
  width: 150%;
  height: 50%;
  position: absolute;
  left: -50%;
  top:0;
  z-index: 1;
  border-radius: 0 0 70% 20% ;
  background: white
}



